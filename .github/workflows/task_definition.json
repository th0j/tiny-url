{
  "family": "demo-tiny-url-fargate",
  "containerDefinitions": [
      {
          "name": "tiny-url",
          "image": "ecr-tiny-url",
          "cpu": 0,
          "portMappings": [
              {
                  "name": "tiny-url-3000-tcp",
                  "containerPort": 3000,
                  "hostPort": 3000,
                  "protocol": "tcp",
                  "appProtocol": "http"
              }
          ],
          "essential": true,
          "environment": [],
          "environmentFiles": [],
          "mountPoints": [],
          "volumesFrom": [],
          "ulimits": [],
          "logConfiguration": {
              "logDriver": "awslogs",
              "options": {
                  "awslogs-create-group": "true",
                  "awslogs-group": "/ecs/demo-tiny-url-fargate",
                  "awslogs-region": "ap-southeast-1",
                  "awslogs-stream-prefix": "ecs"
              },
              "secretOptions": []
          },
          "systemControls": []
      }
  ],
  "executionRoleArn": "arn:aws:iam::363171565563:role/ecsTaskExecutionRole",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
      "FARGATE"
  ],
  "cpu": "256",
  "memory": "512",
  "runtimePlatform": {
      "cpuArchitecture": "X86_64",
      "operatingSystemFamily": "LINUX"
  }
}
